{
  "name": "Wiki Search",
  "version": "1.0",
  "description": "Display information from wikipedia relating to highlighted text",
  "background": {
      "scripts": ["js/js.js"]
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus"
  ],
  "browser_action": {
    "default_popup": "wiki.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/js.js"]
    }
  ],
  "manifest_version": 2
}
